<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyect.Views.Contenpage"
             xmlns:Views="clr-namespace:Proyect.Views"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom">

    <NavigationPage IconImageSource="home.png">
        <x:Arguments>
            <Views:InicioPage/>
        </x:Arguments>
    </NavigationPage>
    
    <NavigationPage IconImageSource="usuario.png">
        <x:Arguments>
            <Views:ClientePage/>
        </x:Arguments>
    </NavigationPage>
    
    <NavigationPage IconImageSource="compra.png">
        <x:Arguments>
            <Views:InventarioPage/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage IconImageSource="compra.png">
        <x:Arguments>
            <Views:SalidaProducto/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage IconImageSource="bloqueado.png">
        <x:Arguments>
            <Views:LoginPage/>
        </x:Arguments>
    </NavigationPage>

</TabbedPage>