<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyect.Views.InventarioPage"
             Title="InventarioPage">

    <VerticalStackLayout Margin="5">

        <Label Text="Inventario" FontSize="30" HorizontalOptions="Center"/>

        <Label Text="Cantidad" FontSize="15"/>
        <Entry />
        <Label Text="Precio" FontSize="15"/>
        <Entry />
        <Label Text="Fecha Venta" FontSize="15"/>
        <Entry />
        <Label Text="Fecha Compra" FontSize="15"/>
        <Entry />
        <Label Text="Fecha de salida" FontSize="15"/>
        <Entry />
        <Label Text="vendedor" FontSize="15"/>
        <Entry />
        <Label Text="Categoria" FontSize="15"/>
        <Picker WidthRequest="200" HorizontalOptions="Start"/>
        <Label Text="QR" FontSize="15"/>
    </VerticalStackLayout>
</ContentPage>